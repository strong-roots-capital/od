{
  "name": "od",
  "version": "0.0.0-development",
  "description": "Oh dear, another date library",
  "main": "lib/src/index.js",
  "types": "lib/src/index.js",
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build",
    "build": "tsc --build .",
    "clean": "rimraf --no-glob lib",
    "ava": "ava -s -v",
    "lint": "eslint --ext json,ts --fix .",
    "lint:check": "eslint --ext json,ts .",
    "test": "nyc ava && npm run size",
    "size": "size-limit",
    "coverage": "nyc report --all --reporter=text-lcov > coverage.lcov && codecov",
    "prettier": "prettier --write src/**/*.ts spec/**/*.ts test/**/*.ts",
    "prettier:check": "prettier --list-different src/**/*.ts spec/**/*.ts test/**/*.ts",
    "lint-staged": "lint-staged",
    "push-with-tags": "git push origin master && git push origin --tags",
    "push-and-publish": "npm run push-with-tags && npm publish",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strong-roots-capital/od.git"
  },
  "keywords": [
    "date"
  ],
  "author": "Eric Crosson <eric.s.crosson@utexas.edu> (https://github.com/strong-roots-capital)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/strong-roots-capital/od/issues"
  },
  "homepage": "https://github.com/strong-roots-capital/od",
  "devDependencies": {
    "@semantic-release/git": "10.0.1",
    "@size-limit/preset-small-lib": "7.0.3",
    "@types/node": "16.11.19",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "ava": "3.15.0",
    "ava-fast-check": "4.0.2",
    "codecov": "3.8.3",
    "eslint": "8.6.0",
    "fast-check": "2.20.0",
    "fp-ts": "2.11.7",
    "lint-staged": "12.1.7",
    "nyc": "15.1.0",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "semantic-release": "18.0.1",
    "size-limit": "7.0.3",
    "tldrlegal": "1.0.11",
    "ts-node": "10.4.0",
    "ts-pattern": "3.3.4",
    "tslib": "2.3.1",
    "typescript": "4.5.4"
  },
  "files": [
    "lib/src"
  ],
  "ava": {
    "files": [
      "!dist/**/*.d.ts",
      "test/**/test-*.ts",
      "spec/**/spec-*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "include": [
      "src/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "lib/src/index.js",
      "limit": "2.50 KB"
    }
  ]
}
