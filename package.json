{
  "name": "od",
  "version": "6.0.1",
  "description": "Oh dear, another date library",
  "main": "index.js",
  "types": "index.js",
  "sideEffects": false,
  "scripts": {
    "benchmark": "ts-node perf/add.ts",
    "build": "tsc --build .",
    "clean": "rimraf --no-glob -- lib",
    "ava": "ava -s -v",
    "licenses:check": "jsgl --local .",
    "lint:fix": "eslint --ext json,ts --fix .",
    "lint": "eslint --ext json,ts .",
    "test": "c8 --include 'src/**/*.ts' ava && npm run size && npm run licenses:check",
    "size": "size-limit",
    "coverage": "c8 report --include 'src/**/*.ts' --all --reporter=text-lcov > coverage.lcov && codecov",
    "format:fix": "prettier --write --ignore-unknown .",
    "format": "prettier --list-different --ignore-unknown .",
    "lint-staged": "lint-staged",
    "semantic-release": "semantic-release",
    "prepublishOnly": "scripts/bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strong-roots-capital/od.git"
  },
  "keywords": [
    "date"
  ],
  "author": "Eric Crosson <eric.s.crosson@utexas.edu> (https://github.com/strong-roots-capital)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/strong-roots-capital/od/issues"
  },
  "homepage": "https://github.com/strong-roots-capital/od",
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.6",
    "@semantic-release/npm": "9.0.1",
    "@size-limit/preset-small-lib": "8.1.0",
    "@types/benchmark": "2.1.2",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "ava": "4.3.3",
    "ava-fast-check": "6.0.0",
    "benchmark": "2.1.4",
    "c8": "7.12.0",
    "codecov": "3.8.3",
    "eslint": "8.25.0",
    "fast-check": "3.2.0",
    "fp-ts": "2.12.3",
    "js-green-licenses": "3.0.1",
    "lint-staged": "13.0.3",
    "microtime": "3.1.1",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.5",
    "size-limit": "8.1.0",
    "ts-node": "10.9.1",
    "ts-pattern": "4.0.5",
    "tslib": "2.4.0",
    "typescript": "4.8.4"
  },
  "files": [
    "*.js",
    "*.js.map",
    "*.d.ts",
    "*.d.ts.map"
  ],
  "ava": {
    "files": [
      "test/**/test-*.ts",
      "spec/**/spec-*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "lib/src/index.js",
      "limit": "2.50 KB"
    }
  ]
}
