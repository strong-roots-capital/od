{
  "name": "od",
  "version": "0.0.0-development",
  "description": "Oh dear, another date library",
  "main": "lib/src/index.js",
  "types": "lib/src/index.js",
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build",
    "build": "tsc --build .",
    "clean": "rimraf --no-glob lib",
    "ava": "ava -s -v",
    "lint": "eslint --ext json,ts .",
    "lint:fix": "eslint --ext json,ts --fix .",
    "test": "nyc ava && npm run size",
    "size": "size-limit",
    "coverage": "nyc report --all --reporter=text-lcov > coverage.lcov && codecov",
    "prettier": "prettier --list-different src/**/*.ts spec/**/*.ts test/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts spec/**/*.ts test/**/*.ts",
    "lint-staged": "lint-staged",
    "push-with-tags": "git push origin master && git push origin --tags",
    "push-and-publish": "npm run push-with-tags && npm publish",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strong-roots-capital/od.git"
  },
  "keywords": [
    "date"
  ],
  "author": "Eric Crosson <eric.s.crosson@utexas.edu> (https://github.com/strong-roots-capital)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/strong-roots-capital/od/issues"
  },
  "homepage": "https://github.com/strong-roots-capital/od",
  "devDependencies": {
    "@semantic-release/git": "^10.0.0",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/node": "^14.11.10",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "ava": "^3.13.0",
    "ava-fast-check": "^4.0.2",
    "codecov": "^3.8.0",
    "eslint": "^7.28.0",
    "fast-check": "^2.5.0",
    "fp-ts": "^2.9.1",
    "lint-staged": "^11.2.3",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^18.0.0",
    "size-limit": "^5.0.3",
    "tldrlegal": "^1.0.11",
    "ts-node": "^9.0.0",
    "ts-pattern": "^1.1.0",
    "tslib": "^2.0.3",
    "typescript": "^4.3.2"
  },
  "files": [
    "lib/src"
  ],
  "ava": {
    "files": [
      "!dist/**/*.d.ts",
      "test/**/test-*.ts",
      "spec/**/spec-*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "include": [
      "src/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "lib/src/index.js",
      "limit": "1.95 KB"
    }
  ]
}
